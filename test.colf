package colfer

// tstObj contains all supported types.
type tstObj struct {
	b   bool
	u32 uint32
	u64 uint64
	i32 int32
	i64 int64
	f32 float32
	f64 float64
	t   timestamp
	s   text
	a   binary
}

// pkg is used for performance benchmarks.
type pkg struct {
	key	int64
	host	text
	addr	binary
	port	int32
	size	int64
	hash	uint64
	ratio	float64
}
